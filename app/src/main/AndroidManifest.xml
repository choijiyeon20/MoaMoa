<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.moamoa">

    <!--서버 외부 접근을 위한 인터넷 사용 허가 설정-->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_moa_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_moa_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoaMoa">
        <activity android:name=".MoAMoA_Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"/>
        <activity android:name=".MoAMoA_Resist"/>
        <activity android:name=".mypage_change_password"/>
        <activity android:name=".mypage_withdraw"/>
        <activity android:name=".Post_comment_Activity"/>
        <activity android:name=".Post_review_Activity"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>