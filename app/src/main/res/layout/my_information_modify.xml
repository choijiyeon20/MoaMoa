<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_mypage">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:orientation="horizontal"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/backButton"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginTop ="7dp"
            app:srcCompat="@drawable/ic_baseline_arrow_back_ios_new_24"
            tools:ignore="DuplicateIds" />

        <ImageView
            android:layout_width="64dp"
            android:layout_height="62dp"
            android:layout_marginStart="100dp"
            app:srcCompat="@drawable/ic_android_black_24dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="404dp"
        android:layout_height="0dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <TextView
            android:layout_marginBottom="20dp"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="110dp"
            android:padding="10dp"
            android:text="Recipe MoA MoA"
            android:textColor="@color/black"
            android:textSize="19dp"
            android:textAlignment="center"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <TextView
            android:layout_marginBottom="2dp"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginStart="80dp"
            android:padding="5dp"
            android:text="내 정보 수정"
            android:textColor="@color/orange"
            android:textSize="40dp"
            android:textAlignment="center"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:layout_width="115dp"
            android:layout_height="115dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="175dp"
            app:srcCompat="@drawable/ic_baseline_group_24" />
        <TextView
            android:layout_marginTop="70dp"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:padding="5dp"
            android:text="코딩모아조"
            android:textColor="@color/black"
            android:textSize="30dp"
            android:textAlignment="center"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout12"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginStart="130dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="85dp"
            android:text="내 게시글"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="80dp"
            android:padding="5dp"
            android:text="장바구니"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="150dp"
        android:layout_height="650dp"
        android:orientation="horizontal"
        android:gravity="center"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="100dp"
            android:layout_height="42dp"
            android:layout_margin="5dp"
            android:layout_marginStart="100dp"
            android:layout_weight="1"
            android:background="@drawable/round"
            android:gravity="center"
            android:text="변경"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="00dp"
        android:layout_height="650dp"
        android:layout_marginLeft="100dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="@+id/linearLayout12"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="280dp"
            android:layout_weight="1"
            android:background="@drawable/round"
            android:gravity="center"
            android:text="3"
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="42dp"
            android:layout_marginTop="475dp"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="300dp"
            android:layout_weight="1"
            android:background="@drawable/round"
            android:gravity="center"
            android:text="검색"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="00dp"
        android:layout_height="650dp"
        android:layout_marginLeft="100dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="@+id/linearLayout12"
        tools:ignore="MissingConstraints"
        tools:context=".mmm">

        <TextView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="280dp"
            android:layout_weight="1"
            android:background="@drawable/round"
            android:gravity="center"
            android:text="2"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginTop="300dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <TextView
            android:layout_marginBottom="2dp"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginStart="0dp"
            android:padding="5dp"
            android:text="별명 수정"
            android:textColor="@color/orange"
            android:textSize="30dp"
            android:textAlignment="center"/>
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:paddingLeft="280dp"
            android:layout_margin="5dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginTop="370dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <TextView
            android:layout_marginBottom="2dp"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginStart="0dp"
            android:padding="5dp"
            android:text="기본 배송지 설정"
            android:textColor="@color/orange"
            android:textSize="30dp"
            android:textAlignment="center"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginTop="420dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">
        <EditText
            android:id="@+id/input_tag"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:paddingLeft="200dp"
            android:layout_margin="5dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="150dp"
        android:layout_height="650dp"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        tools:ignore="MissingConstraints">

    </LinearLayout>

    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginTop="490dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

    </LinearLayout>
    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints">
        <TextView
        android:layout_marginLeft="100dp"
        android:gravity="center_horizontal"
        android:id="@+id/text_kategorie"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginTop="540dp"
        android:layout_weight="1"
        android:background="@drawable/rou"
        android:padding="10dp"
        android:text="저장하기"
        android:textColor="@color/black"
        android:textSize="18dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>