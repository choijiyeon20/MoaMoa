<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Main_mypage">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:layout_width="64dp"
            android:layout_height="62dp"
            android:layout_gravity="center"
            android:layout_marginStart="170dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/big" />

        <ImageView
            android:layout_marginTop="5dp"
            android:layout_width="64dp"
            android:layout_height="50dp"
            android:layout_marginStart="100dp"
            app:srcCompat="@drawable/ic_baseline_favorite_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <TextView
            android:layout_marginBottom="20dp"
            android:layout_marginTop="0dp"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_gravity="center"
            android:layout_marginStart="140dp"
            android:padding="16dp"
            android:text="Recipe MoA MoA"
            android:textColor="@color/black"
            android:textSize="15dp"
            android:textAlignment="center"
            />
        <TextView
            android:layout_marginStart="-165dp"
            android:layout_marginEnd="100dp"
            android:layout_marginTop="13dp"
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Favorites list"
            android:textColor="@color/yellow"
            android:textSize="25dp"
            android:textAlignment="center"
            />
    </LinearLayout>



    <LinearLayout
        android:layout_width="425dp"
        android:layout_height="124dp"
        android:layout_marginTop="83dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout"
        android:background="@color/grey">

        <ImageView
            android:layout_marginTop="18dp"
            android:gravity="center"
            android:layout_width="97dp"
            android:layout_height="62dp"
            android:layout_marginStart="165dp"
            android:background="@color/grey"
            app:srcCompat="@drawable/ic_baseline_favorite_border_24" />

        <TextView
            android:layout_marginTop="6dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="즐겨찾기"
            android:textAlignment="center"
            android:textSize="18dp"
            android:textColor="@color/black"/>

    </LinearLayout>

    <LinearLayout
        android:gravity="center"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="180dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp">

            <ImageButton
                android:id="@+id/mypage"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:srcCompat="@drawable/ic_baseline_local_see_24"
                android:layout_marginStart="20dp"
                android:background="@color/yellow"/>
            <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:srcCompat="@drawable/ic_baseline_post_add_24"
                android:layout_marginStart="20dp"/>
            <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:srcCompat="@drawable/ic_baseline_lightbulb_24"
                android:layout_marginStart="20dp"
                android:background="@color/yellow"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="1dp"
            android:layout_marginTop="1dp">

            <TextView
                android:layout_width="75dp"
                android:layout_height="25dp"
                android:text="요리검색"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginStart="9.11dp" />

            <TextView
                android:layout_width="80dp"
                android:layout_height="25dp"
                android:text="최다리뷰"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginStart="18dp"/>
            <TextView
                android:layout_width="75dp"
                android:layout_height="25dp"
                android:text="마트검색"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginStart="7pt"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="18dp"
            android:layout_marginTop="20dp" >

            <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:srcCompat="@drawable/ic_baseline_person_add_24"
                android:layout_marginStart="40dp"/>

            <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginStart="20dp"
                app:srcCompat="@drawable/ic_baseline_move_up_24"
                android:background="@color/yellow"/>
            <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginStart="18dp"
                app:srcCompat="@drawable/ic_baseline_pin_drop_24" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="1dp">

            <TextView
                android:layout_width="75dp"
                android:layout_height="25dp"
                android:text="추천친구"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginStart="9.11dp" />

            <TextView
                android:layout_width="80dp"
                android:layout_height="25dp"
                android:text="최다방문"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginStart="18dp"/>
            <TextView
                android:layout_width="75dp"
                android:layout_height="25dp"
                android:text="최다태그"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginStart="7pt"/>
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
